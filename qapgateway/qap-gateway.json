{
  "version": 3,
  "name": "bank-api",
  "output_encoding": "no-op",
  "timeout": "10000ms",
  "cache_ttl": "300s",
  "port": 8080,
  "plugin": {
    "pattern": ".so",
    "folder": "/etc/krakend/plugins/"
  },
  "endpoints": [
    {
      "endpoint": "/bank-api/list-users",
      "method": "GET",
      "output_encoding": "no-op",
      "input_query_strings": [],
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET"
          ],
          "expose_headers": [
            "*"
          ],
          "allow_headers": [
            "*"
          ],
          "max_age": "12h",
          "allow_credentials": false
        },
        "proxy": {
          "sequential": true
        },
        "error": {
          "return_status_code": 400,
          "return_body": true
        }
      },
      "backend": [
        {
          "url_pattern": "bank/users",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://688f6e6bf21ab1769f893051.mockapi.io/"
          ],
          "disable_host_sanitize": false,
          "extra_config": {
            "plugin/http-client": {
              "name": "qap-krakend-plugin",
              "gateway_name": "bank-api",
              "endpoint": "/bank-api/list-users"
            }
          }
        }
