{
  "version": 3,
  "name": "bank",
  "output_encoding": "no-op",
  "timeout": "10000ms",
  "cache_ttl": "300s",
  "port": 8080,
  "plugin": {
    "pattern": ".so",
    "folder": "/etc/krakend/plugins/"
  },
  "extra_config": {
    "plugin/http-server": {
      "name": [
        "qap-krakend-plugin"
      ],
      "qap-krakend-plugin": {
        "capture_all": true
      }
    }
  },
  "endpoints": [
    {
      "endpoint": "/bank/api-bank/create-user",
      "method": "POST",
      "output_encoding": "no-op",
      "input_headers": [
        "Authorization",
        "Content-Type"
      ],
      "input_query_strings": [],
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET"
          ],
          "expose_headers": [
            "*"
          ],
          "allow_headers": [
            "*"
          ],
          "max_age": "12h",
          "allow_credentials": false
        },
        "proxy": {
          "sequential": true
        },
        "error": {
          "return_status_code": 400,
          "return_body": true
        },
        "auth/validator": {
          "alg": "RS256",
          "jwk_url": "",
          "propagate_claims": [
            [
              "sub",
              "X-User"
            ],
            [
              "realm_access.roles",
              "X-Roles"
            ]
          ],
          "propagate_headers": [
            "Authorization"
          ]
        },
        "qos/ratelimit/router": {
          "max_rate": 5,
          "client_max_rate": 5,
          "every": "1m",
          "strategy": "ip",
          "burst": 5
        }
      },
      "backend": [
        {
          "url_pattern": "bank/users",
          "encoding": "json",
          "sd": "static",
          "method": "POST",
          "host": [
            "https://688f6e6bf21ab1769f893051.mockapi.io/"
          ],
          "disable_host_sanitize": false,
          "extra_config": {
            "plugin/http-client": {
              "name": "qap-krakend-plugin",
              "gateway_name": "bank",
              "endpoint": "/bank/api-bank/create-user"
            }
          }
        }
      ],
      "input_body": "*"
    },
    {
      "endpoint": "/bank/api-bank/list-users",
      "method": "GET",
      "output_encoding": "no-op",
      "input_headers": [
        "Authorization"
      ],
      "input_query_strings": [],
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET"
          ],
          "expose_headers": [
            "*"
          ],
          "allow_headers": [
            "*"
          ],
          "max_age": "12h",
          "allow_credentials": false
        },
        "proxy": {
          "sequential": true
        },
        "error": {
          "return_status_code": 400,
          "return_body": true
        },
        "auth/validator": {
          "alg": "RS256",
          "jwk_url": "",
          "propagate_claims": [
            [
              "sub",
              "X-User"
            ],
            [
              "realm_access.roles",
              "X-Roles"
            ]
          ],
          "propagate_headers": [
            "Authorization"
          ]
        },
        "qos/ratelimit/router": {
          "max_rate": 5,
          "client_max_rate": 5,
          "every": "1m",
          "strategy": "ip",
          "burst": 5
        }
      },
      "backend": [
        {
          "url_pattern": "bank/users",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://688f6e6bf21ab1769f893051.mockapi.io/"
          ],
          "disable_host_sanitize": false,
          "extra_config": {
            "plugin/http-client": {
              "name": "qap-krakend-plugin",
              "gateway_name": "bank",
              "endpoint": "/bank/api-bank/list-users"
            }
          }
        }
      ]
    }
  ]
}