{
  "version": 3,
  "name": "dogs",
  "output_encoding": "no-op",
  "timeout": "5000ms",
  "cache_ttl": "300s",
  "port": 8080,
  "plugin": {
    "pattern": ".so",
    "folder": "/etc/krakend/plugins/"
  },
  "endpoints": [
    {
      "endpoint": "/dogs/dogs-fotos",
      "method": "GET",
      "output_encoding": "no-op",
      "input_headers": [
        "Authorization"
      ],
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET"
          ],
          "expose_headers": [
            "*"
          ],
          "allow_headers": [
            "*"
          ],
          "max_age": "12h",
          "allow_credentials": false
        },
        "proxy": {
          "sequential": true
        },
        "error": {
          "return_status_code": 400,
          "return_body": true
        }
      },
      "backend": [
        {
          "url_pattern": "/api/breeds/image/random",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://dog.ceo"
          ],
          "disable_host_sanitize": false,
          "extra_config": {
            "plugin/http-client": {
              "name": "qap-krakend-plugin",
              "gateway_name": "dogs",
              "endpoint": "/dogs/dogs-fotos"
            }
          }
        }
      ]
    }
  ]
}