{
  "version": 3,
  "name": "bank-pet",
  "output_encoding": "no-op",
  "timeout": "10000ms",
  "cache_ttl": "300s",
  "port": 8080,
  "plugin": {
    "pattern": ".so",
    "folder": "/etc/krakend/plugins/"
  },
  "endpoints": [
    {
      "endpoint": "/bank-pettest2/swagger-petstore/pet/{petId}",
      "method": "GET",
      "output_encoding": "no-op",
      "input_headers": [
        "Authorization"
      ],
      "input_query_strings": [],
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false
        },
        "proxy": {
          "sequential": true
        },
        "error": {
          "return_status_code": 400,
          "return_body": true
        },
        "auth/validator": {
          "alg": "RS256",
          "jwk_url": "https://idp.konneqt.cloud/realms/qap-local/protocol/openid-connect/certs",
          "propagate_claims": [
            [
              "sub",
              "X-User"
            ],
            [
              "realm_access.roles",
              "X-Roles"
            ]
          ],
          "propagate_headers": [
            "Authorization"
          ]
        },
        "qos/ratelimit": {
          "max_rate": 1,
          "client_max_rate": 1,
          "strategy": "ip"
        }
      },
      "backend": [
        {
          "url_pattern": "/v2/pet/{petId}",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "cache_ttl": "300s",
          "host": [
            "https://petstore.swagger.io"
          ],
          "disable_host_sanitize": false,
          "extra_config": {
            "plugin/http-client": {
              "name": "qap-krakend-plugin",
              "gateway_name": "bank-pet",
              "endpoint": "/bank-pettest2/swagger-petstore/pet/{petId}"
            }
          }
        }
      ]
    },
