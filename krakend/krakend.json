{
  "version": 3,
  "name": "api-deckofcards",
  "timeout": "10000ms",
  "cache_ttl": "300s",
  "output_encoding": "no-op",
  "port": 3890,
  "extra_config": {},
  "endpoints": [
    {
      "endpoint": "/api-deckofcards/deck/new/shuffle",
      "method": "GET",
      "output_encoding": "no-op",
      "input_query_strings": [
        "deck_count"
      ],
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false
        }
      },
      "backend": [
        {
          "url_pattern": "/api/deck/new/shuffle",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "cache_ttl": "300s",
          "host": [
            "https://deckofcardsapi.com"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}